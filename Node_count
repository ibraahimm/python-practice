class Node():
    def __inint__(self, value, next):
        self.data = value
        self.next = next
        
    def next_value(self):
        return self.next
        
node5 = Node(10, None)
node4 = Node(5, node5)
node3 = Node(1, node4)
node2 = Node(9, node3)
head  = Node(6, node2)

def count_node(first)
    current = first
    counter = 0
    while current:
        counter +=1
        current = current.next_value()
        print(counter)
        
 
count_node(head)
    
